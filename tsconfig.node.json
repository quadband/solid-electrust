  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["electron-vite/node"]
  },
  "include": ["electron.vite.config.*", "app/src/main/**/*", "app/src/preload/**/*", "native/**/*"]
}
